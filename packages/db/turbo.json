{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"pipeline": {
		"db:generate": {
			"cache": false,
			"inputs": ["prisma/schema.prisma"]
		},
		"db:run-seed": {
			"dependsOn": ["download:seedData"]
		},
		"download:seedData": {
			"outputs": ["seed/data/downloaded/*.json"]
		},
		"type-check": {}
	}
}
